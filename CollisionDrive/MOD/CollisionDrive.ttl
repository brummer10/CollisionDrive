
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:   <http://xmlns.com/foaf/0.1/> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.
@prefix opts:   <http://lv2plug.in/ns/ext/options#> .
@prefix time:   <http://lv2plug.in/ns/ext/time#>.
@prefix units:  <http://lv2plug.in/ns/extensions/units#> .
@prefix atom:   <http://lv2plug.in/ns/ext/atom#> .
@prefix urid:   <http://lv2plug.in/ns/ext/urid#> .
@prefix pprop:  <http://lv2plug.in/ns/ext/port-props#> .
@prefix midi:   <http://lv2plug.in/ns/ext/midi#> .
@prefix patch:  <http://lv2plug.in/ns/ext/patch#> .


<urn:name#me>
   a foaf:Person ;
   foaf:name "brummer10" .

<https://github.com/brummer10/CollisionDrive>
   a lv2:Plugin ,
       lv2:DistortionPlugin ;
   doap:maintainer <urn:name#me> ;
   doap:name "CollisionDrive" ;
   lv2:project <https://github.com/brummer10/CollisionDrive> ;
   lv2:requiredFeature urid:map ;
   lv2:optionalFeature lv2:hardRTCapable ;
   lv2:minorVersion 1 ;
   lv2:microVersion 0 ;

rdfs:comment """
...
""";


   lv2:port  [
       a lv2:AudioPort ,
          lv2:OutputPort ;
      lv2:index 0 ;
      lv2:symbol "out" ;
      lv2:name "out" ;
   ], [
       a lv2:AudioPort ,
          lv2:InputPort ;
      lv2:index 1 ;
      lv2:symbol "in" ;
      lv2:name "in" ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 2 ;
      lv2:designation lv2:enabled;
      lv2:portProperty lv2:toggled ;
      lv2:symbol "BYPASS" ;
      lv2:name "bypass" ;
      lv2:default 1 ;
      lv2:minimum 0 ;
      lv2:maximum 1 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 3 ;
      lv2:symbol "BRIGHT" ;
      lv2:name "bright" ;
      lv2:default 0.500000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 1.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 4 ;
      lv2:symbol "GATE" ;
      lv2:name "gate" ;
      lv2:default -55.000000 ;
      lv2:minimum -95.000000 ;
      lv2:maximum -10.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 5 ;
      lv2:symbol "ATTACK" ;
      lv2:name "attack" ;
      lv2:portProperty lv2:integer;
      lv2:default 3.000000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 5.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 6 ;
      lv2:symbol "DRIVE" ;
      lv2:name "drive" ;
      lv2:default 0.100000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 1.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 7 ;
      lv2:symbol "LEVEL" ;
      lv2:name "level" ;
      lv2:default -10.000000 ;
      lv2:minimum -20.000000 ;
      lv2:maximum 4.000000 ;
   ], [
      a lv2:OutputPort ,
          lv2:ControlPort ;
      lv2:index 8 ;
      lv2:symbol "V1" ;
      lv2:name "v1" ;
      lv2:default 0.000000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 1.000000 ;
   ] .

